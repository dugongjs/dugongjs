    "author": "Erik Lysne",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "dugong",
        "dugongjs"
    ],
    "type": "module",
    "exports": {
        ".": "./src/index.ts"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false
    },
    "scripts": {
        "test:typecheck": "tsc --noEmit",
        "test:e2e:run": "DEBUG=testcontainers* vitest run --config vitest.config.e2e.ts",
        "test:e2e:watch": "DEBUG=testcontainers* vitest watch --ui --config vitest.config.e2e.ts",
        "lint": "eslint .",
        "build": "vite-node ./scripts/build.ts",
        "prepublishOnly": "pnpm build"
    },
    "peerDependencies": {
        "@dugongjs/core": "workspace:*",
        "@dugongjs/nestjs": "workspace:*",
        "@nestjs/common": "^10.4.13",
        "@nestjs/core": "^10.4.13",
        "@nestjs/microservices": "^10.4.13",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@dugongjs/eslint-config": "workspace:*",
        "@dugongjs/nestjs-typeorm": "workspace:*",
        "@dugongjs/scripts": "workspace:*",
        "@dugongjs/typeorm": "workspace:*",
        "@dugongjs/typescript-config": "workspace:*",
        "@faker-js/faker": "^8.4.1",
        "@nestjs/platform-express": "^10.4.17",
        "@nestjs/testing": "^10.4.17",
        "@nestjs/typeorm": "^10.0.2",
        "@swc/core": "^1.11.21",
        "@swc/helpers": "^0.5.17",
        "@testcontainers/postgresql": "^10.24.2",
        "@types/node": "^20.17.6",
        "@types/pg": "^8.11.14",
        "@types/supertest": "^6.0.3",
        "@types/uuid": "^9.0.8",
        "@vitest/ui": "^3.1.2",
        "dotenv": "^16.5.0",
        "nestjs-pino": "^4.4.0",
        "pg": "^8.15.6",
        "pino": "^9.6.0",
        "reflect-metadata": "^0.2.2",
        "supertest": "^7.1.0",
        "typeorm": "^0.3.22",
        "typescript": "^5.6.3",
        "unplugin-swc": "^1.5.2",
        "vite-node": "^3.1.2",
        "vitest": "^3.1.2",
        "vitest-mock-extended": "^2.0.2",
        "zod": "^3.24.3"
