{
    "name": "@dugongjs/cli",
    "version": "0.0.24",
    "author": "Erik Lysne",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0"
    },
    "keywords": [
        "dugong",
        "dugongjs"
    ],
    "type": "module",
    "exports": {
        ".": "./src/index.ts"
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist",
        "linkDirectory": false
    },
    "bin": {
        "dugong": "./index.js"
    },
    "scripts": {
        "dugong": "vite-node ./src/index.ts",
        "lint": "eslint .",
        "build": "vite-node ./scripts/build.ts",
        "prepublishOnly": "pnpm build"
    },
    "dependencies": {
        "@dugongjs/core": "workspace:*",
        "@dugongjs/nestjs": "workspace:*",
        "@dugongjs/nestjs-microservice-query": "workspace:*",
        "@nestjs/common": "^10.4.13",
        "@nestjs/core": "^10.4.13",
        "@nestjs/microservices": "^10.4.13",
        "@nestjs/platform-express": "^10.4.13",
        "@tanstack/react-query": "^5.89.0",
        "chalk": "^5.4.1",
        "commander": "^13.1.0",
        "figures": "^6.1.0",
        "fullscreen-ink": "^0.1.0",
        "ink": "^6.3.0",
        "ink-select-input": "^6.2.0",
        "ink-text-input": "^6.0.0",
        "jsondiffpatch": "^0.7.3",
        "react": "^19.1.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@dugongjs/eslint-config": "workspace:*",
        "@dugongjs/scripts": "workspace:*",
        "@dugongjs/typescript-config": "workspace:*",
        "@types/node": "^24.5.1",
        "@types/react": "^19.1.13",
        "typescript": "^5.6.3",
        "vite-node": "^3.1.2"
    }
}
